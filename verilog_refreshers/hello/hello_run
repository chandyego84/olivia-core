#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_012C4B80 .scope module, "hello_tb" "hello_tb" 2 3;
 .timescale -9 -9;
v012C40A0_0 .var "A", 0 0;
v012C40F8_0 .net "B", 0 0, L_012C4C08; 1 drivers
S_012C1668 .scope module, "uut" "hello" 2 8, 3 1, S_012C4B80;
 .timescale -9 -9;
L_012C4C08 .functor BUFZ 1, v012C40A0_0, C4<0>, C4<0>, C4<0>;
v012C16F0_0 .net "A", 0 0, v012C40A0_0; 1 drivers
v012C1748_0 .alias "B", 0 0, v012C40F8_0;
    .scope S_012C4B80;
T_0 ;
    %vpi_call 2 11 "$dumpfile", "hello_tb.vcd";
    %vpi_call 2 14 "$dumpvars", 1'sb0, S_012C4B80;
    %set/v v012C40A0_0, 0, 1;
    %delay 20, 0;
    %load/v 8, v012C40A0_0, 1;
    %inv 8, 1;
    %set/v v012C40A0_0, 8, 1;
    %delay 20, 0;
    %load/v 8, v012C40A0_0, 1;
    %inv 8, 1;
    %set/v v012C40A0_0, 8, 1;
    %delay 20, 0;
    %vpi_call 2 25 "$display", "Test completed.";
    %end;
    .thread T_0;
    .scope S_012C4B80;
T_1 ;
    %vpi_call 2 30 "$monitor", "A=%d, B=%d\012", v012C40A0_0, v012C40F8_0;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "hello_tb.v";
    "hello.v";
